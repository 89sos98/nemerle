using Nemerle.Collections.List;

module M {
  Main () : void {
    def l = ["foo", "bar", "baz"];
    mutable acc <- ":" ;
    def prt (s : string) : void {
        acc <- acc + ":" + s;
        printf ("%s\n", s) 
    };
    Iter (prt, l);
    printf ("%s\n", acc);
    def concat (acc : string, s : string) : string { acc + "." + s };
    printf ("%s\n", FoldLeft (concat, "", l))
  }
}

/*
BEGIN-OUTPUT
foo
bar
baz
::foo:bar:baz
.foo.bar.baz
END-OUTPUT
*/
