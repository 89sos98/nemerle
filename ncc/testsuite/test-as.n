open Nemerle.Collections;
module M {
  public Main () : void {
    def l = [1;2;3;4];
    match (l) {
      | (_) as x :: _ => print_endline (string_of_int (x))
      | _ => ()
    };
    match (l) {
      | (_ :: _) as k =>
        print_endline (string_of_int (List.length (k)));
        print_endline (string_of_int (k.hd))
      | _ => ()
    };
    match ([]) {
      | (_ :: _) as k =>
        print_endline (string_of_int (k.hd))
      | _ => ()
    }
  }
}

(*
BEGIN-OUTPUT
1
4
1
END-OUTPUT
*)
