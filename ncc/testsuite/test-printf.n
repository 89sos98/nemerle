
module M {
  public Main () : void {
    printf ("Ala %d ma %s i %s\n", 101010, "koat", "paes");
    printf ("Dziala%s czy %d nie %d %s\n", " mi to", 3, 4, "dziala?");
 
    mutable i <- 1;
    mutable n <- 1;
    while (i < 6) {
      printf("licze %d\n", n);
      i <- i + 1;
      n <- i * n;
    };

    mutable teststr <- "";
    teststr <- sprintf (teststr, "Dynamicznie %s string o %s %d i z %d numerkiem\nA to druga %s\n",
             "generowany", "dlugosci", teststr.Length, 3, "linijka");
    System.Console.Write (teststr);
  }
}

