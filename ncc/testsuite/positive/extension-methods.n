#pragma indent
using System.Console

class A
  public v : int
  public this (v : int)
    this.v = v
  public foo () : void
    Write ("foo()\n")

class B
  public static foo (this a : A, p : int) : void
    Write ($"foo($(a.v),$p)\n")

def a = A (42)
a.foo ()
a.foo (1)
B.foo (a, 2)

/*
BEGIN-OUTPUT
foo()
foo(42,1)
foo(42,2)
END-OUTPUT
*/
