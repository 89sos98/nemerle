using Nemerle.IO;

module A {

  g (x : ref int) : void
  {
    x *= 7;
  }

  f (x : ref int, y : out object) : void
  {
    x = x + 3;
    g (ref x);
    y = 42;
  }

  Main () : void
  {
    mutable x = 3;
    mutable y = null;
    f (ref x, out y);
    printf ("%d %s\n", x, y.ToString ());
  }
}

/*
BEGIN-OUTPUT
42 42
END-OUTPUT
*/
