// REFERENCE: System.dll

open System;

public class ArrayTest
{
        static make (s : string) : void {
          regexp (match (s) {
            | "a*" => printf ("1\n");
            | "b+.*" => printf ("2\n");
            | @"(?<first : Int32>\d+)-\d*" => printf ("%d 3\n", first);
            | "(?<a>(ala|kasia))? ma kota" =>
              match (a) {
                | None => printf ("samo ma kota\n")
                | Some (s) => printf ("%s ma kota\n", s)
              }
            | _ => printf ("default\n");
          });
        }

	public static Main () : void 
	{
          make ("aaaaa");
          make ("bbbbbla");
          make ("c");
          make ("23432-334");
          make ("22-");
          make (" ma kota");
          make ("ala ma kota");
	}
}

