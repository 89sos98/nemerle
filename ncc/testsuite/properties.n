using Nemerle.IO;

class C {
  mutable foo : int;
  
  public Foo : int
  {
    get { foo }
    set { foo <- value }
  }

  public this (x : int) { foo <- x; }
}


module M {
  Main () : void
  {
    def x = C (4);
    printf ("%d\n", x.Foo);
    x.Foo <- 2;
    printf ("%d\n", x.Foo);
  }
}
/*
BEGIN-OUTPUT
4
2
END-OUTPUT
*/
