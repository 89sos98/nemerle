
public class ArrayTest
{
	public static Main () : void 
	{
		print_endline ("Array test.");

		mutable a <- mkarray [1; 2; 3];
		mutable b <- mkarray ["ala"; "ma"; "kota"];

		mutable x <- 2;
		mutable y <- 3;

		mutable c <- mkarray [x; y; x + y; x - y; if (x < y) x else y];

		mutable r0 <- c [2];

		c [2] <- 1979;

		mutable r1 <- c [2];

		printf ("%dd ", r0);

		printf ("%d\n", r1);

		()
	}
}

