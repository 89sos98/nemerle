using Nemerle.Collections;
module M {
  public Main () : void {
    def l = [1,2,3,4];
    match (l) {
      | (_) as x :: _ => printf ("%d\n", x)
      | _ => ()
    };
    match (l) {
      | (_ :: _) as k =>
        printf ("%d\n", List.Length (k));
        printf ("%d\n", k.hd)
      | _ => ()
    };
    match ([]) {
      | (_ :: _) as k =>
        printf ("%d\n", k.hd)
      | _ => ()
    }
  }
}

/*
BEGIN-OUTPUT
1
4
1
END-OUTPUT
*/
