module M {

  runner (f : void -> void) : void
  {
    f ()
  }
  
  Main () : void {

    mutable x <- (42 : object);
    printf ("%d\n", (x :> int));
    x <- 6 * 9;
    printf ("%d\n", (x :> int));
    x <- "foo";
    printf ("%s\n", (x :> string));

    mutable y <- (420 : object);
    printf ("%d\n", (y :> int));
    
    def f () {
      y <- 6 * 9 + 1;
      printf ("%d\n", (y :> int));
      y <- "bar";
      printf ("%s\n", (y :> string));
    };

    runner (f);

   def x = (1 : object);
   def tab = array [("ala" : object), "kot", x];
   printf ("%d\n", tab.Length);

  }
}

