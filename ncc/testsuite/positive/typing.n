// bug #789
interface I1{ f1(x:int):int; }
interface I2{ f2(x:int):int; }

class M1 : I1, I2
{
  public f1(x:int):int {x+1}
  public f2(x:int):int {x+2}
}

class M2 : I1, I2
{
  public f1(x:int):int {x-1}
  public f2(x:int):int {x-2}
}

def x1 = 5;
def Processors = [M1() : I2,M2() : I2];

def y = Processors.Map( m  =>
  match(m) {
    | m is M1 => m.f1(x1)
    | m is M2 => m.f2(x1)
    | _ => x1
  } );

System.Console.WriteLine($"y = $y"); 


/*
BEGIN-OUTPUT
y = [6, 3]
END-OUTPUT
*/
