using Nemerle.Collections;

class Adder {
  val : int;
  public m (x : int) : void { System.Console.WriteLine (x + val) }
  public m2 () : void { List.Iter (m, [10;11;12]); }
  public this (x : int) { val <- x; }
}

module M {
  public Main () : void {
    def l = [1;2;3];
    def a1 = Adder (1);
    def a2 = Adder (2);
    List.Iter (a1.m, l);
    List.Iter (a2.m, l);
    a1.m2();
    a2.m2();
  }
}

