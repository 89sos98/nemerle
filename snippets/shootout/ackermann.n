using System;
using Nemerle.IO;

class App {
  public static Main(args : array<string>) : int {
    mutable n = Convert.ToInt32(args[0]);
    when (n < 1) n = 1;
      
    def Ack (M, N) {
      | (0, _) => N + 1
      | (_, 0) => Ack (M - 1, 1)
      | _ => Ack (M - 1, Ack (M, (N - 1)))
    };
    printf ("Ack(3,%d): %d\n", n, Ack (3, n));
    0
  }
}
