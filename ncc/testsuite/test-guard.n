module M {
  f (x : int * int) : string {
    match (x) {
      | (1, 2) => "1,2"
      | (2, 3)
      | (3, 2) => "2,3"
      | (x, 12) when x < 50 => string_of_int (x)
      | (x, _) when x > 10 => "+10"
      | (x, 3) when x > 3 => "+3,3"
      | (x, y) when x > y 
      | (1, 55) => "x>y"
      | _ => "?"
    }
  }

  test (x : int, y : int) : void {
    print_endline (f (x, y))
  }

  public Main () : void {
    test (1, 2);
    test (2, 3);
    test (3, 2);
    test (11, 7);
    test (9, 7);
    test (4, 3);
    test (0, 3);
    test (14, 12);
    test (33, 12);
    test (50, 12);
    
    test (1, -1);
    test (1, 55);
  }
}

