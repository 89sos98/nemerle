
public class ArrayTest
{
  public static Main () : void 
  {
    printf ("Array test.\n");

    mutable _a <- mkarray [1; 2; 3];
    mutable _b <- mkarray ["ala"; "ma"; "kota"];

    mutable x <- 2;
    mutable y <- 3;

    mutable c <- mkarray [x; y; x + y; x - y; if (x < y) x else y];

    mutable r0 <- c [2];

    c [2] <- 1979;

    mutable r1 <- c [2];

    printf ("%d\n%d\n", r0, r1);

    def x = mkarray (20);
    x[5] <- 7;
    def x = mkarray (20);
    x[5] <- "foo";
    ()
  }
}

