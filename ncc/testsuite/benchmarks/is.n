using Nemerle.Diagnostics;

interface I { }
class A : I { }


module Test {
  is_interface (x : object) : void {
    mutable cond = true;
    for (mutable i = 0; i < 100000000; ++i)
      cond = cond && (x is I);
  }

  is_type (x : object) : void {
    mutable cond = true;
    for (mutable i = 0; i < 100000000; ++i)
      cond = cond && (x is A);
  }

  
  Main () : void {
    time is_interface (A());
    time is_type (A());
  }
}

