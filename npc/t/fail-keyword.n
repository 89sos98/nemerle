open T;

class T {
  static f1 (i : int, s : string) : void {}

  static t1 () : void = f1 (i = 1, "foo");          // E: keyword parameters must come last
  static t2 () : void = f1 (i = "foo", s = "foo");  // E: typing error in function call: type clash, needed int, got string in arg #1
  static t3 () : void = f1 (i = 1, s = 1);   // E: typing error in function call: type clash, needed string, got int in arg #2
  static t4 () : void = f1 (s = "foo", i = "foo"); // E: typing error in function call: type clash, needed int, got string in arg #1
  static t5 () : void = f1 (s = 1, i = 1);  // E: typing error in function call: type clash, needed string, got int in arg #2
  static t6 () : void = f1 (s = "foo", i = 1);  // OK
  static t7 () : void = f1 (1, s = "foo");  // OK
  static t8 () : void = f1 (1, "foo");  // OK
  static t9 () : void = f1 (1, i = 1);  // E: argument was already consumed
  static t10 () : void = f1 (1, i = 1, s = "foo");  // E: needed 2 arguments in call, got 3
  static t11 () : void = f1 (foo = 1, i = 1); // E: function has no parameter named `foo'
}
