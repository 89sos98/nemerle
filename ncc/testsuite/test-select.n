open Nemerle.Collections;

module M {
  public Main () : void {
    def a = ("a","b","c","d","e","f","g");
    def b = selectFromTuple (4, 7, a);
    printf ("%s\n", b);

    def 'a decompose (l : list ('a)) : 'a * 'a {
      (List.hd (l), List.last (l))
    };
    def tp = tupleMap (decompose, ([3;2;7], ["gfd"; "e3rr"; "b2"]));
    pretty_print_expr (printf ("ka%d%smil", 5, "ss"));
    def ((a,b), (c,d)) = tp;
    printf ("%d %d %s %s\n", a, b, c, d);
  }
}

(*
BEGIN-OUTPUT
d
3 7 gfd b2
END-OUTPUT
