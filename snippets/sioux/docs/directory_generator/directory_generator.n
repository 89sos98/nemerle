using Sioux;
using Nemerle.Xml;
using System.Xml;
using System.Xml.Xsl;

public class Dirgen : Application
{
    public this ()
    {
        def xsl = XslTransform ();
        xsl.Load ("styles/dir2xhtml.xsl", null);
        def dg = DirectoryGenerator(".");
        FormTemplate = Some(XmlPipeline.Process(dg,[],[xsl]));
    }
}