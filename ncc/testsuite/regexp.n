// REFERENCE: System.dll

using System;

public class ArrayTest
{
        static make (s : string) : void {
          regexp (match (s) {
            | "a*.+" => printf ("a\n");
            | @"(?<num : int>\d+)-\w+" => printf ("%d\n", num + 3);
            | "(?<name>(Ala|Kasia))? ma kota" =>
              match (name) {
                | None => printf ("noname?\n")
                | Some (n) => printf ("%s\n", n)
              }
            | _ => printf ("default\n");
          })
        }

	public static Main () : void 
	{
          make ("aaaaa");
          make ("bbbbbla");
          make ("c");
          make ("23432-334");
          make ("22-");
          make (" ma kota");
          make ("ala ma kota");
	}
}

