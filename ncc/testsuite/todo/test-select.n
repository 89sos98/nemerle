open Nemerle.Collections;

module M {
  public Main () : void {
    def a = ("a","b","c","d","e","f","g");
    def b = selectFromTuple (4, 7, a);
    printf ("%s\n", b);


    def decompose (l) {
      (List.hd (l), List.last (l))
    };
    def tp = tupleMap (decompose, ([3;2;5;2;7;3], ["gfd"; "sew"; "asw"; "e3rr"; "b2"]));
    def ((a,b), (c,d)) = tp;
    printf ("%d %d %s %s\n", a, b, c, d);
  }
}

