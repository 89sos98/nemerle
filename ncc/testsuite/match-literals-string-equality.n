module X {

public variant option ('a)
  {
    | None
    | Some { val : 'a; }
  }

  public Main () : void {
    def x = "foo3";
    def y = "foo" + string_of_int (3);

    match (Some (x)) {
      | Some ("foo3") => print_endline ("OK")
      | _ => ()
    };

    match (Some (y)) {
      | Some ("foo3") => print_endline ("OK")
      | _ => ()
    };

  }

}

(*
BEGIN-OUTPUT
OK
OK
END-OUTPUT
*)
