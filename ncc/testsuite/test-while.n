module M {
  public Main () : void {
    mutable i <- 5;
    while (i > 0) { printf ("%d\n", i); i <- i - 1; };

    for (mutable i <- 0; i < 5; i <- i + 1) {
      printf("for %d\n", i);
    };
    for (mutable j <- 3; j > 0; j <- j - 1) {
      printf ("f %d\n", j);
    };

    def arr = array (5);
    arr[3] <- 4;
    foreach (i : int in arr) {
      printf ("%d", i + 1);
    };
    printf ("\n");
  }
}

