using Nemerle.Diagnostics;

class C {
  public static f1 (x : int) : int
  {
     f2 (x + 1)
  }
  public static f2 (x : int) : int
  {
     if (x < 100000)
       f1 (x + 1)
     else
       x
  }

  public static Main () : void
  {
    mutable res = 0;
    
    time {
      for (mutable i = 0; i < 5000; ++i)
        res += f1 (0);
    }    
  }
}
