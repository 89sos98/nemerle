abstract class C1 {
  abstract public m1 () : void;
  public m2 () : void
  {
    m1 ();
  }
}

class C2 extends C1 {
  public override m1 () : void
  {
    System.Console.WriteLine ("C2.m1");
  }

  public this () {}
}

module M {
  Main () : void 
  {
    def c = C2 ();
    c.m1 ();
    (c : C1).m1 ();
    c.m2 ();
    (c : C1).m2 ();
  }
}

