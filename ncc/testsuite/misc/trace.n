
using Nemerle.IO;

module A {

  [Nemerle.Diagnostics.Trace (printf ("executing %s:%d:%s ()\n", file, line, method))]
  public m () : void {
    def x = 5;
    mutable y = 4;
    for (mutable i = 0; i < x; ++i) {
      y = x * 3 + y;
      y = 2 * y;
      y *= i;
    }
    y -= 1;
  }


  Main () : void {
    m ();
  }
}
