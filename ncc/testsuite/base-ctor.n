class X {
  public this (m : string) {
    printf ("X %s\n", m)
  }
  public this () {
    printf ("X()\n")
  }
}

class Y : X {
  public this (m : string) {
    base ();
    printf ("Y %s\n", m);
  }
  public this () {
    base ("foobar");
    printf ("Y()\n");
  }
  public this (_ : int) {
    base ();
    printf ("Y(int)\n");
  }
}

variant foo : Y {
  | F_1 { f1 : string; }
}

module M {
  public Main () : void {
    def _ = X ("foo");
    def _ = X ();
    def _ = Y ("foobarbaz");
    def _ = Y ();
    def _ = Y (3);
    def _ = F_1 ("foo");
    def _ = F_1 ("bux", "foo");
    ()
  }
}

