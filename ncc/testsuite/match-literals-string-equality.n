module X {


  public Main () : void {
    def x = "foo3";
    def y = "foo" + string_of_int (3);

    match (Some (x)) {
      | Some ("foo3") => print_endline ("OK/A")
      | _ => ()
    };

    match (Some (y)) {
      | Some ("foo3") => print_endline ("OK/B")
      | _ => ()
    };

    when (x == y) {
      print_endline ("OK/C")
    };

  }

}

(*
BEGIN-OUTPUT
OK/A
OK/B
OK/C
END-OUTPUT
*)
