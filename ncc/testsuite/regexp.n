// NO-TEST
public class ArrayTest
{
        static make (s : string) : void {
          regexp (match (s) {
            | "a*" => printf ("1\n");
            | "b+.*" => printf ("2\n");
            | @"(?<first>\d+)-\d*" => printf ("%s 3\n", first);
            | _ => printf ("default\n");
          });
        }

	public static Main () : void 
	{
          mutable s <- System.Text.RegularExpressions.Regex (@"(^(a(b))(c(d)(?<kamil>(e)*))$)|(?<kamil>^now$)(?<kamil>b)|(^nor$)");
          def tex = "abcdeee";
//          def tex = "nowb";
//          def tex = "nor";
          def m = s.Match(tex);
          def empty = "1";
          def q = System.Convert.ToInt32 (empty);
          printf ("%d\n", q);
          printf("%s\n", m.Groups["kamil"].ToString());
          for (mutable i <- 0; i < m.Groups.Count; i <- i + 1) {
            printf ("%s\n", m.Groups[i].ToString ());
          };

          def namesreg = System.Text.RegularExpressions.Regex (@"\(\?\<(\w+)\s*(:\s*(\w+))?");
          mutable m <- namesreg.Match 
                   (@"(?<ida>(^(a(b))(c(d)(?<kamil : int>(e)*))$)|(?<kamil>^now$)(?<kamil : Company>b)|(^nor$)");
          while (m.Success) {
            printf ("///////////////\n");
            for (mutable i <- 0; i < m.Groups.Count; i <- i + 1) {
              printf ("%s\n", m.Groups[i].ToString ());
            };

            m <- m.NextMatch ();
          };
          printf ("aa %d\n", System.Int32.Parse ("34"));

          make ("aaaaa");
          make ("bbbbbla");
          make ("c");
          make ("23432-334");
          make ("22-");
	}
}

