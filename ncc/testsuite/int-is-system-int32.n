
  class M
  {
    static immut_x : int;
    static this () { immut_x <- 3 }

    static Main () : void {
    mutable x <- (123 : int);
    mutable y <- (321 : System.Int32);

    y <- x;    

    System.Console.WriteLine ("{0} {1} {2}",
        int.Parse ("132"), System.Int32.Parse ("132"), y);

    def t1 = 10 %| 12;
    def t2 = 10 %& 12;
    System.Console.WriteLine ("{0} {1}", t1, t2);
    
    }
  }

