using System;

class X {
	static  mutable  i : int;
	static  mutable  j : int;
	
	static m () :  void
	{
		i = 0;
		j = 0;
		
		try {
			throw  ArgumentException ("Blah");
		}
		
		catch { |  _ : ArgumentException => {
			i = 1;
		} |  _ : Exception => {
			i = 2;
		}
		
		} finally {
			j = 1;
		}
	}

	static ret ( a : int) :  int
	{
		try {
			when (a == 1)
				throw  Exception ();
			
			 1;
		}
		catch { | _ : System.Exception =>  {
			 2;
		}
		}
	}
	
	static Main () :  int
	{
		m ();
		if (i != 1)
		{
			 1;
		}
		else
	{
		if (j != 1)
		{
			 2;
		}
		else
	{

		if (ret (1) != 2)

		{
			 3;

		}

		else
	{

		if (ret (10) != 1)

		{
			 4;

		}

		else
	{
		
		 0;
	}
	}
	}
	}
	}
}
