module M {
  public Main () : void {
    mutable i <- 5;
    while (i > 0) { print_endline (string_of_int (i)); i <- i - 1; };

    for (mutable i <- 0; i < 5; i <- i + 1) {
      printf("for %d\n", i);
    };
    for (mutable j <- 3; j > 0; j <- j - 1) {
      printf ("f %d\n", j);
    };
    
    def 'a f () : 'a {
      def pararr = (System.Array.CreateInstance (typeof (System.Object), 5)
      :> array ('a));
      pararr[0];
    };
    f();

//    if (true) 1 else ();
  }
}

