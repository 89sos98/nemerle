module Foo {

  foo ['a] (a : 'a) : 'a
    {
      def bar ['b] (a : 'a, b : 'b) : 'a * 'b {
        ((a : 'a), (b : 'b))
      };
      def (x, y) = bar (a, 1);
      def _ = x;
      def _ = y;
      def (x, y) = bar (a, "qux");
      def _ = y;
      x
    }

  Main () : void {
    assert (1 == foo (1));
    assert (A (object()).ret () == null);
  }
}


class A ['a] where 'a : class {
  public this (_ : 'a) { }
  
  public ret () : 'a {
    null
  }
}


