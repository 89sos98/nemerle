variant Tree<'a> {
  | Node { left : Tree<'a>; mid : 'a; right : Tree<'a>; }
  | Leaf

  public override ToString () : string
  {
    match (this) {
      | Node (l, m, r) =>
        "(" + l.ToString () + " " + m.ToString () + " " + r.ToString () + ")"
      | Leaf => "."
    }
  }
}

module M {
  Main () : void
  {
    def tr = Node (Node (Leaf (), 1, Leaf ()), 2, Leaf ());
    System.Console.WriteLine (tr.ToString ());
  }
}

