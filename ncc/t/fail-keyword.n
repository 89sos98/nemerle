open T;

class T {
  static f1 (i : int, s : string) : void {}

  static t1 () : void = f1 (i = 1, "foo");          // E: unnamed parameters after named
  static t2 () : void = f1 (i = "foo", s = "foo");  // E: in arg .1 needed .*int got .*string
  static t3 () : void = f1 (i = 1, s = 1);   // E: in arg .2 needed .*string got .*int
  static t4 () : void = f1 (s = "foo", i = "foo"); // E: in arg .1 needed .*int got .*string
  static t5 () : void = f1 (s = 1, i = 1);  // E: in arg .2 needed .*string got .*int
  static t6 () : void = f1 (s = "foo", i = 1);  // OK
  static t7 () : void = f1 (1, s = "foo");  // OK
  static t8 () : void = f1 (1, "foo");  // OK
  static t9 () : void = f1 (1, i = 1);  // E: .*
  static t10 () : void = f1 (1, i = 1, s = "foo");  // E: needed 2 parameters, got 3
  static t11 () : void = f1 (foo = 1, i = 1); // E: .*
  // FIXME: messages for .* arn't perfect yet
}
