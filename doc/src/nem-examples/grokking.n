fib (n : int) : int
{
  mutable last1 = 1;
  mutable last2 = 1;
  
  for (mutable cur = 1; cur < n; ++cur) {
    def tmp = last1 + last2;
    last1 = last2;
    last2 = tmp;
  }

  last2
}
