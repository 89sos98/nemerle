using Nemerle.IO;


public class ArrayTest
{
    public static Main () : void 
    {
        printf ("Hashtables test.\n");

        mutable a = System.Collections.Hashtable ();

        a.Add ("helloworld", "Hello World, my name is not Jan B.");

        if (a.Contains ("helloworld"))
            printf ("Hello world!\n")
        else
            printf ("Goodbye cruel world!\n");

        printf ("Have %d elements in the hashtable.\n", a.Count);

        printf ("%s\n", (a ["helloworld"] :> System.String));
        
        ()
    }
}

