
variant list ('a)
  {
    | Cons { hd : 'a; tl : list ('a); }
    | Nil {}
  }


public class ListCompareTest {

    public static Main () : void
	{
	    mutable li <- [];

	    if ( li == [] )
	        print_endline ("empty-list")
	    else
	        print_endline ("compare-failed")
	}
}

