module M {
  intize (s : string) : int {
    match (s) {
      | "eins" => 1
      | "zwei" => 2
      | "drei" => 3
      | _ => 0
    }
  }

  stringize (i : int) : string {
    match (i) {
      | 1 => "odin"
      | 2 => "dwa"
      | 3 => "tri"
      | 4 => "czjetyrie"
      | _ => "?"
    }
  }

  alfabetize (c : char) : string {
    match (c) {
      | 'a' => "aj"
      | 'b' => "bi"
      | 'z' => "zet"
      | _ => "??"
    }
  }

  string_of_bool (x : bool) : string {
    match (x) {
      | true => "true"
      | false => "false"
    }
  }

  print_int (i : int) : void { print_string (string_of_int (i) + "\n") }

  Main () : void {
    print_int (intize ("eins"));
    print_int (intize ("zwei"));
    print_int (intize ("vier"));
    print_endline (stringize (1));
    print_endline (stringize (2));
    print_endline (stringize (4));
    print_endline (stringize (0));
    print_endline (alfabetize ('a'));
    print_endline (alfabetize ('b'));
    print_endline (alfabetize ('c'));
    print_endline (string_of_bool (true));
    print_endline (string_of_bool (false));
  }
}

